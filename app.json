{
  "expo": {
    "name": "KickOffHub",
    "slug": "KickOffHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash":{
      "image": "./assets/images/logo-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#005B41"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "notifications": {
      "icon": "./assets/images/icon.png",
      "color": "#005B41"
    },
    "plugins": [
      "expo-router",
      ["expo-secure-store", { "facefaceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data." }],
      ["expo-notifications", { "icon": "./assets/images/icon.png", "color": "#005B41"}]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
